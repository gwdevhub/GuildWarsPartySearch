{
  "IpRateLimitOptions": {
    "EnableEndpointRateLimiting": false,
    "StackBlockedRequests": false,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 20
      },
      {
        "Endpoint": "*",
        "Period": "10m",
        "Limit": 100
      },
      {
        "Endpoint": "*",
        "Period": "12h",
        "Limit": 3000
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "EnvironmentOptions": {
    "Name": "Release"
  },
  "ServerOptions": {
    "ApiKey": "[APIKEY]",
    "Certificate": "[CERTIFICATE_BASE64]",
    "InactivityTimeout": "0:0:15",
    "HeartbeatFrequency": "0:0:1"
  },
  "StorageAccountOptions": {
    "ConnectionString": "[AZURE_TABLESTORAGE_CONNECTIONSTRING]"
  },
  "PartySearchTableOptions": {
    "TableName": "searches"
  },
  "ContentOptions": {
    "ContainerName": "content",
    "UpdateFrequency": "0:1:0",
    "StagingFolder": "Content"
  }
}